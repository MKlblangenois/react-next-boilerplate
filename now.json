{
  "version": 2,
  "name": "react-next-boilerplate",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    { 
      "src": "/favicon.ico", 
      "dest": "/static/favicon.ico" 
    },
    { 
      "src": "/static/(.*)", 
      "headers": { 
        "cache-control": "s-maxage=31536000,immutable" 
      }, 
      "dest": "/static/$1" 
    },
    {
      "src": "/(.*)$",
      "dest": "/index?slug=$1"
    }
  ]
}
